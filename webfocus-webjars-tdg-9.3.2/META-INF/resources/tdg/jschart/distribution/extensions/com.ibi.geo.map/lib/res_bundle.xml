<ibx-res-bundle name="test_res_bundle.xml" loadContext="bundle">
	<strings>
		<string-file src="{res}/com.ibi.intl.ibxtools.wfcomponents.multilayermap.wfc_multilayermap" />	
    </strings> 
	<style>
		 <style-file src="../css/geo_map.css"/>
	</style>
    <scripts>
        <script-block>
            <script-file src="./geo_ui_overlay.js"/>
            <script-file src="./ibgeo.js"/>
            <![CDATA[
				//# sourceURL=mapping_res.xml
			]]>
        </script-block>
    </scripts>
    <markup>
    	<markup-block>  
    		<!-- Widget to show messages within containers -->
			<div class="wfc-map-container-message map-container-message" data-ibx-type="ibxVBox" aria-disabled="false" data-ibx-name="containerMessage">
			 	<div class="messageTypeWrapper" data-ibx-type="ibxVBox" data-ibx-name="messageTypeWrapper">
					<div class="messageIconWrapper" data-ibx-type="ibxVBox"  data-ibx-name="messageTypeWrapper1">
						<div class="messageIcon" data-ibx-name="messageIcon" data-ibx-type="ibxLabel" data-ibxp-glyph-classes="ibx-icons ds-icon-layers"></div>
					</div>
					<div class="messageTextWrapper" data-ibx-type="ibxVBox" data-ibx-name="messageTextWrapper">
						<div aria-disabled="false" data-ibx-type="ibxLabel" data-ibxp-text-align="left" data-ibxp-justify="start" data-ibxp-text-wrap="true" class="messageLabel" data-ibx-name="messageLabel"></div>
						<div aria-disabled="false" class="messageDetails" data-ibx-name="messageDetails"></div>
					</div>
				</div>
			</div>
		</markup-block>
    	<markup-block>        	
            <div class="map-layout-obj map-container-frame" data-ibx-type="ibxVBox" data-ibxp-align="stretch">
            	<div class="map-layout-obj map-container-tbar" data-ibx-type="ibxHBox" data-ibxp-align="center">
                  <div tabindex="0" data-ibxp-command = "cmdToggleTOC" class="btn-sel-type btnToggleTOC" title="@ibxString('mlmap.Layers')" data-ibxp-glyph-classes="ibx-icons ds-icon-layers"  data-ibx-type="ibxButton"></div>
                  <div tabindex="0" data-ibxp-command = "cmdToggleLE" class="btn-sel-type btnToggleLE" title="@ibxString('mlmap.Legend')" data-ibxp-glyph-classes="ibx-icons ds-icon-map-legend"  data-ibx-type="ibxButton"></div>
                  <div tabindex="0" data-ibxp-command = "cmdToggleBM" class="btn-sel-type btnToggleBM" title="@ibxString('mlmap.Switch_Basemap')" data-ibxp-glyph-classes="ibx-icons ds-icon-map"  data-ibx-type="ibxButton"></div>                        
                  <div tabindex="0" data-ibxp-command = "cmdToggleView" class="btn-sel-type btnToggleView" title="@ibxString('mlmap.viewType')" data-ibx-type="ibxButton">3D</div>
               <div class="separator actions" data-ibx-type="ibxHBox">
                	<div tabindex="0" data-ibxp-command = "cmdRemoveCustom" class="btn-sel-type btnRemoveCustom" title="@ibxString('mlmap.reset')" data-ibxp-glyph-classes="ibx-icons ds-icon-reset" data-ibx-type="ibxButton"></div>
                	<div tabindex="0" data-ibxp-command = "cmdlocate" class="btn-sel-type btnlocate esri-icon esri-icon-locate" title="@ibxString('mlmap.locate')" data-ibx-type="ibxButton"></div>                     
             	</div>    
             	 <div class="separator actions" data-ibx-type="ibxHBox">
                	<div tabindex="0" data-ibxp-command = "cmdSaveCustom" class="btn-sel-type btnSaveCustom" title="@ibxString('mlmap.bookmarks')"  data-ibxp-glyph-classes="ibx-icons ds-icon-bookmark"  data-ibx-type="ibxButton"></div> 
                	<div tabindex="0" data-ibxp-command = "cmdToggleTimeSlider" class="btn-sel-type btnTimeSlider" title="@ibxString('mlmap.timeslider')"  data-ibxp-glyph-classes="ibx-icons ds-icon-slider"  data-ibx-type="ibxButton"></div> 
                	<div tabindex="0" data-ibxp-command = "cmdToggleScalerangeSlider" class="btn-sel-type btnScalerangeSlider" title="@ibxString('mlmap.scalerangeslider')"  data-ibxp-glyph-classes="ibx-icons ds-icon-slider-range"  data-ibx-type="ibxButton"></div>                       
             	</div>    
              <div class="separator interactionTools" data-ibx-type="ibxHBox">
              	<div tabindex="0" data-ibxp-command = "cmdDiscover" class="btn-sel-type btnDiscover" title="@ibxString('mlmap.discover')"  data-ibxp-glyph-classes="ibx-icons ds-icon-magic"  data-ibx-type="ibxButton"></div>
               	<div tabindex="0" data-ibxp-command = "cmdsearch" class="btn-inter-type btnsearch esri-icon-search" title="@ibxString('mlmap.search')" data-ibx-type="ibxButton"></div>
	            <div tabindex="0" data-ibxp-command= "cmdmeasurement" class="btn-inter-type btnmeasurement esri-icon-measure-line" title="@ibxString('mlmap.measurement')"  data-ibx-type="ibxButton"></div>                        
	            <div tabindex="0" data-ibxp-command = "cmddirection" class="btn-inter-type btndirection esri-icon-directions" title="@ibxString('mlmap.direction')" data-ibx-type="ibxButton"></div>
	            <div tabindex="0" data-ibxp-command = "cmdlocation" class="btn-inter-type btnlocation esri-icon esri-icon-locate-circled" title="@ibxString('mlmap.location')" data-ibx-type="ibxButton"></div>
	            <div tabindex="0" data-ibxp-command = "cmdSketch" class="btn-inter-type btnsketch esri-icon ds-icon-edit" title="@ibxString('mlmap.sketch')" data-ibx-type="ibxButton"></div>  	
                
                  </div>  
                  <div class="separator selTools" data-ibx-type="ibxHBox">  
                      <div tabindex="0" class="cmdRadioGroup" data-ibx-type="ibxHRadioGroup" data-ibxp-command="cmdSelType" data-ibx-name="rgSelType" data-ibxp-align="center">                        
                          <div tabindex="0" class="btn-sel-type sel-extent" data-ibx-type="ibxRadioButton" title="@ibxString('mlmap.RectSel')" data-ibxp-justify="center" data-ibxp-glyph-classes="ibx-icons" data-ibxp-user-value="selExtent"></div>
                          <div tabindex="0" class="btn-sel-type sel-circle" data-ibx-type="ibxRadioButton" title="@ibxString('mlmap.RadSel')" data-ibxp-justify="center" data-ibxp-glyph-classes="ibx-icons" data-ibxp-user-value="selCircle"></div>                        
                          <div tabindex="0" class="btn-sel-type sel-polygon" data-ibx-type="ibxRadioButton" title="@ibxString('mlmap.FreehandSel')" data-ibxp-justify="center" data-ibxp-glyph-classes="ibx-icons" data-ibxp-user-value="selPolygon"></div>
						<div tabindex="0" class="btn-sel-type sel-custpolygon" data-ibx-type="ibxRadioButton" title="@ibxString('mlmap.CustomSel')" data-ibxp-justify="center" 
							data-ibxp-glyph-classes="ibx-icons" data-ibxp-user-value="selCustPolygon"></div>    
                          <div tabindex="0" class="btn-sel-type sel-pan" data-ibx-type="ibxRadioButton" data-ibxp-justify="center" data-ibxp-user-value="selPan"></div> 
                      </div>                     
                  </div>                  
              </div>
            	<div id="com-ibi-geo-map" class="map-layout-obj map-container"  data-ibx-type="ibxWidget"></div>
            </div>   
    	</markup-block>
    </markup>
</ibx-res-bundle>