		<profile>
			<id>whitesource-js</id>
			<properties>
				<whitesource.configFile>${project.basedir}/wss-unified-agent.config</whitesource.configFile>
				<whitesource.scanFolder>${project.basedir}/src/main/resources/META-INF/resources</whitesource.scanFolder>
				<whitesource.project>${project.artifactId}_${project.version}</whitesource.project>
				<whitesource.product>WebFOCUS_Maven</whitesource.product>
				<wss.unified.agent.version>22.6.1</wss.unified.agent.version>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>org.codehaus.mojo</groupId>
						<artifactId>exec-maven-plugin</artifactId>
						<version>3.0.0</version>
						<executions>
							<execution>
								<phase>verify</phase>
								<goals>
									<goal>java</goal>
								</goals>
							</execution>
						</executions>
						<configuration>
							<includeProjectDependencies>false</includeProjectDependencies>
							<includePluginDependencies>true</includePluginDependencies>
							<mainClass>org.whitesource.fs.Main</mainClass>
							<arguments>
