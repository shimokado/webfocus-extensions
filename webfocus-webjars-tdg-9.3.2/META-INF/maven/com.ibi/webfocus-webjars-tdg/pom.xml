	<profiles>
		<profile>
			<id>compile-css-javascript</id>
			<properties>
				<frontend.maven.plugin.version>1.11.2</frontend.maven.plugin.version>
			</properties>
			<build>
				<plugins>
					<plugin>
						<groupId>com.github.eirslett</groupId>
						<artifactId>frontend-maven-plugin</artifactId>
						<version>${frontend.maven.plugin.version}</version>
						<executions>
							<execution>
								<id>grunt-install-node-and-npm</id>
								<goals>
									<goal>install-node-and-npm</goal>
								</goals>
							</execution>
							<execution>
								<id>grunt-npm-config-cache</id>
								<phase>generate-resources</phase>
								<goals>
									<goal>npm</goal>
								</goals>
								<configuration>
									<arguments>config set cache ${project.basedir}/npm-cache -L project</arguments>
								</configuration>
							</execution>
							<execution>
								<id>npm-config-ssl</id>
								<phase>generate-resources</phase>
								<goals>
									<goal>npm</goal>
								</goals>
								<configuration>
									<arguments>config set strict-ssl false -L project</arguments>
								</configuration>
							</execution>
							<execution>
								<id>grunt-npm-install</id>
								<phase>generate-resources</phase>
								<goals>
									<goal>npm</goal>
								</goals>
								<configuration>
									<arguments>install --production</arguments>
								</configuration>
							</execution>
							<execution>
								<id>grunt-build</id>
								<phase>compile</phase>
								<goals>
									<goal>grunt</goal>
								</goals>
								<configuration>
									<arguments>--verbose --pkg_version=${project.version}</arguments>
								</configuration>
							</execution>
						</executions>
						<configuration>
							<nodeVersion>v16.13.0</nodeVersion>
							<npmVersion>8.1.3</npmVersion>
							<installDirectory>${basedir}</installDirectory>
							<workingDirectory>${basedir}</workingDirectory>
							<nodeDownloadRoot>https://na1prdfocnxs01.tibco.com:8443/repository/nodejs-org/</nodeDownloadRoot>
							<npmDownloadRoot>https://na1prdfocnxs01.tibco.com:8443/repository/npm-all/npm/-/</npmDownloadRoot>
						</configuration>
					</plugin>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-antrun-plugin</artifactId>
						<version>1.8</version>
						<executions>
							<execution>
								<id>closure-compile</id>
								<phase>compile</phase>
								<goals>
									<goal>run</goal>
								</goals>
								<configuration>
									<target>
										<property name="fileset.dir" value="${project.build.directory}/classes/META-INF/resources" />
										<property name="target" value="${project.build.directory}/classes/META-INF/resources/tdg/jschart/distribution" />
										<copy todir="${target}/extensions">
											<fileset dir="${fileset.dir}/tdg/jschart/js.source/src/extensions" />
										</copy>
										<copy todir="${target}/leaflet">
											<fileset dir="${fileset.dir}/tdg/jschart/js.source/src/leaflet" />
										</copy>
										<copy todir="${target}/moonbeam_server">
											<fileset dir="${fileset.dir}/tdg/jschart/js.source/src/moonbeam_server" />
										</copy>
										<copy todir="${target}/map">
											<fileset dir="${fileset.dir}/tdg/jschart/js.source/src/map" />
										</copy>
									</target>
								</configuration>
							</execution>
							<execution>
								<id>ant-compress</id>
								<phase>prepare-package</phase>
								<goals>
									<goal>run</goal>
								</goals>
								<configuration>
									<target>
										<taskdef resource="net/sf/antcontrib/antlib.xml" />
										<property name="fileset.dir" value="${project.build.directory}/classes/META-INF/resources" />
										<echo message="fileset.dir: ${fileset.dir}" />
										<for param="file" parallel="false">
											<path>
												<fileset dir="${fileset.dir}">
													<include name="**/*.min.js" />
													<include name="**/*-min.js" />
													<include name="**/*_min.js" />
													<include name="**/*.css" />
													<include name="**/*.xml" />
												</fileset>
											</path>
											<sequential>
												<echo message="file: @{file}" />
												<property name="source" value="@{file}" />
												<property name="target" value="@{file}.gz" />
												<gzip src="${source}" destfile="${target}" />
												<var name="source" unset="true" />
												<var name="target" unset="true" />
											</sequential>
										</for>
									</target>
								</configuration>
							</execution>
						</executions>
						<dependencies>
							<dependency>
								<groupId>ant-contrib</groupId>
								<artifactId>ant-contrib</artifactId>
								<version>1.0b3</version>
								<exclusions>
									<exclusion>
										<groupId>ant</groupId>
										<artifactId>ant</artifactId>
									</exclusion>
								</exclusions>
							</dependency>
							<dependency>
								<groupId>org.apache.ant</groupId>
								<artifactId>ant-nodeps</artifactId>
								<version>1.8.1</version>
							</dependency>
						</dependencies>
					</plugin>
					<plugin>
						<groupId>org.apache.maven.plugins</groupId>
						<artifactId>maven-jar-plugin</artifactId>
						<version>${maven.jar.plugin.version}</version>
						<configuration>
							<excludes>
								<exclude>**/js.source/**</exclude>
							</excludes>
						</configuration>
					</plugin>
				</plugins>
			</build>
		</profile>
